<Window x:Class="SalaryFond.Views.Windows.ResultCompaniesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SalaryFond.Views.Windows"
        mc:Ignorable="d"
        Title="ResultCompaniesWindow" Height="450" Width="800"
        DataContext="{Binding ResultCompaniesViewModel, Source={StaticResource Locator}}">
    <Grid>
        <ListBox ItemsSource="{Binding Companies}">
            <ListBox.ItemTemplate>
                <DataTemplate DataType="m:Company">
                    <DockPanel>
                        
                        <TextBlock Text="{Binding Name}"/>
                    </DockPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</Window>
